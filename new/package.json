{
	"name": "@ankylos/preset-next",
	"version": "0.2.0",
	"description": "Next.JS preset for anklyos",
	"keywords": [
		"typescript",
		"nextjs",
		"react"
	],
	"homepage": "https://github.com/safinsingh/ankylos",
	"license": "MPL-2.0",
	"author": {
		"name": "Safin Singh",
		"email": "safinsingh.dev@gmail.com",
		"url": "https://safin.dev"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/safinsingh/ankylos",
		"directory": "/ankylos-preset-next"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"fmt": "prettier --write '*/**/*.{js,ts,tsx,yml,scss,json}'",
		"fmt:ci": "prettier --check '*/**/*.{js,ts,tsx,yml,scss,json}'",
		"lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
		"lint:ci": "eslint '*/**/*.{js,ts,tsx}' --quiet"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"next": "^10.0.5",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"sass": "^1.32.5"
	},
	"devDependencies": {
		"@ankylos/eslint-config": "workspace:^0.2.0",
		"@ankylos/prettier-config": "workspace:^0.2.0",
		"@ankylos/template-editorconfig": "workspace:^0.2.0",
		"@ankylos/template-eslint": "workspace:^0.2.0",
		"@ankylos/template-github": "workspace:^0.2.0",
		"@ankylos/template-husky": "workspace:^0.2.0",
		"@ankylos/template-markdownlint": "workspace:^0.2.0",
		"@ankylos/template-pnpm": "workspace:^0.2.0",
		"@ankylos/template-prettier": "workspace:^0.2.0",
		"@ankylos/template-renovate": "workspace:^0.2.0",
		"@ankylos/template-vscode": "workspace:^0.2.0",
		"@types/node": "^14.14.22",
		"@types/react": "^17.0.0",
		"eslint": "^7.18.0",
		"prettier": "^2.2.1",
		"typescript": "^4.1.3"
	}
}
